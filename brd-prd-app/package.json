{
  "name": "brd-prd-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run env:validate && next dev",
    "dev:3001": "npm run env:validate && next dev --port 3001",
    "dev:range": "node scripts/start-with-port-range.js",
    "dev:domain": "node scripts/setup-local-domain.js && npm run dev:range",
    "open": "node scripts/open-local-domain.js",
    "build": "npm run env:validate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node test-debug.js",
    "test:debug-simple": "jest --detectOpenHandles --forceExit",
    "test:single": "jest --maxWorkers=1 --detectOpenHandles",
    "test:verbose": "jest --verbose --no-coverage",
    "test:isolate": "node test-debug.js --runInBand --bail=1",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma migrate reset",
    "env:validate": "node scripts/validate-env.js",
    "env:setup": "cp .env.example .env.local",
    "deploy:production": "node scripts/deploy-production.js",
    "seed:production": "node scripts/seed-production.js",
    "postinstall": "prisma generate",
    "predev": "npm run env:validate",
    "prebuild": "npm run env:validate"
  },
  "dependencies": {
